{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && cp .nojekyll dist/",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lintfix": "prettier --write --list-different . && yarn lint:js --fix",
    "deploy": "gh-pages -d dist -t true"
  },
  "dependencies": {
    "@nuxt/content": "^2.12.1",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/critters": "^0.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nuxt": "^3.10.2",
    "prettier": "^3.2.5",
    "sass": "^1.71.0",
    "vue": "^3.4.19",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "eslint": "^8.56.0",
    "gh-pages": "^6.1.1",
    "nuxt-contextual-transition": "^0.1.3",
    "nuxt-delay-hydration": "^1.3.3",
    "nuxt-marquee": "^1.0.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.0.2"
  }
}
